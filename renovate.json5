{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekly",
    "group:allNonMajor"
  ],
  "labels": [
    "dependencies"
  ],
  "ignorePaths": [
    "**/typescript/**",
    "**/node_modules/**",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/dist/**",
    "**/coverage/**",
    "/yonode.org/**",
    "/docs.yonode.org/**"
  ],
  "rangeStrategy": "bump",
  "reviewers": ["isasharafdin"]
}
